	<body>
		<div style="display: flex; justify-content: space-between">
			<h3>{{ "invoicePdf.qapline_name"|trans }}</h3>
			<h3>{{ "invoicePdf.invoice_number"|trans }}
				{{varSymbol}}</h3>
		</div>
		<table class="formatted-table" style="width: 100%; border: solid 1px; border-collapse: collapse">
			<tr>
				<td class="td" rowspan="2" style="width: 40%; border: solid 1px;">
					<p class="p0">
						<b>{{ "invoicePdf.supplier"|trans }}</b>
					</p>
					<div style="margin: auto; width: fit-content;">
						<p class="p0">QapLine spol.s r.o.</p>
						<p class="p0">Tř. Tomáše Bati 299</p>
						<p class="p0">760 01 Zlín</p>
						<p class="p1">{{ "invoicePdf.contact.vat_number"|trans }}: 29316880</p>
						<p class="p0">{{ "invoicePdf.contact.cin_number"|trans }}: CZ29316880</p>
						<p class="p0">{{ "invoicePdf.contact.mobile"|trans }}: 774005555</p>
						<p class="p0">{{ "invoicePdf.contact.email"|trans }}: qapline@qapline.com</p>
					</div>
				</td>
				<td class="td" style="width: 60%; border: solid 1px">
					<div style="display: flex; justify-content: space-between" class="p-1">
						<p class="p0">{{ "invoicePdf.variable_symbol"|trans }}:</p>
						<p class="p0">{{varSymbol}}</p>
					</div>
					<div style="display: flex; justify-content: space-between" class="p-1">
						<p class="p0">{{ "invoicePdf.constant_symbol"|trans }}:</p>
						<p class="p0">{{constSymbol}}</p>
					</div>
					<div style="display: flex; justify-content: space-between" class="p-1">
						<p class="p0">{{ "invoicePdf.order_number"|trans }}:
							{{orderNumber}}</p>
						<p class="p0">{{orderDate}}</p>
					</div>
				</td>
			</tr>
			<tr>
				<td class="td" rowspan="2" style="border: solid 3px">
					<table style="width: 100%">
						<tr>
							<td style="width: 50%">{{ "invoicePdf.customer"|trans }}:</td>
							<td style="width: 25%">{{ "invoicePdf.vat_number"|trans }}:</td>
							<td style="text-align: right; width: 25%">{{registrationNumber}}</td>
						</tr>
						<tr>
							<td style="width: 50%"></td>
							<td style="width: 25%">{{ "invoicePdf.cin"|trans }}:</td>
							<td style="text-align: right; width: 25%">{{taxId}}</td>
						</tr>
					</table>
					<table style="width: 100%">
						<tr>
							<td>
								<h3 class="p0">{{customerCompany}}</h3>
							</td>
						</tr>
						<tr>
							<td>
								<b>{{customerStreet}}</b>
							</td>
						</tr>
						<tr>
							<td>
								<b>
									{{customerCountry}}-{{customerPostalCode}}
									{{customerCity}}
								</b>
							</td>
						</tr>
					</table>
				</td>
			</tr>
			<tr>
				<td class="td" style="border: solid 3px">
					<table style="width: 100%">
						<tr>
							<td>{{ "invoicePdf.bank"|trans }}:</td>
							<td colspan="3"></td>
						</tr>
						{% if currency == "EUR" %}
							<tr>
								<td>{{ "invoicePdf.iban"|trans }}:</td>
								<td colspan="3">CZ8520100000002400374721</td>
							</tr>
							<tr>
								<td>{{ "invoicePdf.swift"|trans }}:</td>
								<td colspan="3">FIOBCZPPXXX</td>
							</tr>
						{% endif %}
						<tr>
							<td>{{ "invoicePdf.account_number"|trans }}:</td>
							{% if currency == "EUR" %}
								<td>2400374721</td>
							{% else %}
								<td>107-3934450287</td>
							{% endif %}
							<td>{{ "invoicePdf.bank_code"|trans }}:</td>
							<td>{{ currency == "CZK" ? "0100" : "8300" }}</td>
						</tr>
					</table>
				</td>
			</tr>
			<tr>
				<td class="td" style="border: solid 1px">
					<table style="width: 100%; border-collapse: collapse">
						<tr>
							<td>{{ "invoicePdf.exposure_date"|trans }}:</td>
							<td style="text-align: right; border: solid 1px">{{exposureDate}}</td>
						</tr>
						<tr>
							<td>{{ "invoicePdf.maturity_date"|trans }}:</td>
							<td style="text-align: right; border: solid 1px">{{maturityDate}}</td>
						</tr>
						<tr>
							<td>{{ "invoicePdf.performance_date"|trans }}:</td>
							<td style="text-align: right; border: solid 1px">{{performanceDate}}</td>
						</tr>
						<tr>
							<td>{{ "invoicePdf.payment_form"|trans }}:</td>
							<td style="text-align: right">{{("invoicePdf.payment_" + paymentMethod)|trans}}</td>
						</tr>
					</table>
				</td>
				<td class="td" style="border: solid 1px">
					<p class="p0">{{ "invoicePdf.recipient"|trans }}:</p>
				</td>
			</tr>
			<tr>
				<td class="td py-3" colspan="2" style="border: solid 1px">
					<p class="p0">{{ "invoicePdf.delivery_facturation"|trans }}</p>
				</td>
			</tr>
			<tr>
				<td class="td" style="padding-inline: 0px" colspan="2">
					<table style="width: 100%; border-collapse: collapse">
						<tr class="tr-r-5">
							<td style="white-space: nowrap; border-bottom: solid 1px">{{ "invoicePdf.delivery_id"|trans }}:</td>
							<td style="white-space: nowrap; border-bottom: solid 1px">{{ "invoicePdf.order_id"|trans }}</td>
							<td style="white-space: nowrap; border-bottom: solid 1px">{{ "invoicePdf.order_date"|trans }}</td>
							<td style="white-space: nowrap; border-bottom: solid 1px">{{ "invoicePdf.loading_date"|trans }}</td>
							<td style="white-space: nowrap; border-bottom: solid 1px;text-align: right">{{ "invoicePdf.price"|trans }}</td>
							<td style="white-space: nowrap; border-bottom: solid 1px;text-align: right;">{{ "invoicePdf.vat_percent"|trans }}</td>
							<td style="white-space: nowrap; border-bottom: solid 1px;text-align: right">{{ "invoicePdf.vat"|trans }}</td>
							<td style="white-space: nowrap; border-bottom: solid 1px;text-align: right">{{ "invoicePdf.total"|trans }}</td>
						</tr>
						{% for commission in commissions %}
							<tr class="tr-r-5 tr-p-1">
								<td>{{commission.text}}</td>
								<td>{{commission.orderedBy|trans_source}}</td>
								<td>{{commission.orderDate}}</td>
								<td>{{commission.loadingDate}}</td>
								<td style="text-align: right">{{commission.price|format_number}}</td>
								<td style="text-align: right">{{commission.vat}}</td>
								<td style="text-align: right">{{commission.priceVat|format_number}}</td>
								<td style="text-align: right">{{commission.priceWithVat|format_number}}</td>
							</tr>
						{% endfor %}
						<tr class="tr-r-5 tr-t-10">
							<td colspan="4">{{ "invoicePdf.items_sum"|trans }}</td>
							<td style="text-align: right">{{price|format_number}}</td>
							<td></td>
							<td style="text-align: right">{{priceVat|format_number}}</td>
							<td style="text-align: right">{{priceWithVat|format_number}}</td>
						</tr>
						{% if currency == "EUR" %}
							<tr class="tr-r-5">
								<td colspan="4">{{ "invoicePdf.rounding"|trans }}</td>
								<td colspan="4" style="text-align: right">{{rounding|format_number}}</td>
							</tr>
						{% endif %}
						<tr class="tr-r-5 tr-t-10">
							<td>{{ "invoicePdf.total_to_pay"|trans }}</td>
							<td colspan="7">
								<h3 class="p0" style="text-align: right">
									{{priceTotal|format_number}}
									{{currency}}
								</h3>
							</td>
						</tr>
					</table>
				</td>
			</tr>
			<tr>
				<td class="td" colspan="2" style="border: solid 1px">
					<table>
						<tr>
							<td>{{ "invoicePdf.issued_by"|trans }}:</td>
							<td>{{createdBy}}</td>
						</tr>
						<tr>
							<td></td>
							<td style="text-align: right">{{createdByMail}}</td>
						</tr>
					</table>
					<p class="p0" style="margin-top: 10px">
						{{ "invoicePdf.issuer_cin_message"|trans }}
					</p>
				</td>
			</tr>
			<tr>
				<td class="td" colspan="2" style="border: solid 1px">
					<table style="width: 100%;">
						<tr>
							<td>
								{% if qrCode %}
									<picture>
										<img style="height: 150px; width: 150px;" src="data:image/png;base64,{{qrCode}}" alt="qr_code"/>
									</picture>
								{% else %}
									<picture>
										<img style="height: 150px; width: 150px;" src="data:image/svg+xml;base64," alt="qr_code"/>
									</picture>
								{% endif %}
							</td>
							<td>
								<table style="width: 100%;">
									<tr>
										<td>{{ "invoicePdf.vat_recapitulation"|trans }}:</td>
										<td style="text-align: right;">{{ ("invoicePdf.basis_in_" + currency)|trans }}</td>
										<td style="text-align: right;">{{ "invoicePdf.vat_rate"|trans }}</td>
										<td style="text-align: right;">{{ ("invoicePdf.vat_in_" + currency)|trans }}</td>
										<td style="text-align: right;">{{ ("invoicePdf.total_with_vat_in_" + currency)|trans }}</td>
									</tr>
									<tr>
										<td></td>
										<td style="text-align: right;">{{priceNone|format_number}}</td>
										<td style="text-align: right;">0%</td>
										<td style="text-align: right;">0,00</td>
										<td style="text-align: right;">{{priceNone|format_number}}</td>
									</tr>
									<tr>
										<td></td>
										<td style="text-align: right;">{{priceHigh|format_number}}</td>
										<td style="text-align: right;">21%</td>
										<td style="text-align: right;">{{priceVatHigh|format_number}}</td>
										<td style="text-align: right;">{{priceHighWithVat|format_number}}</td>
									</tr>
									{% if currency == "EUR" %}
										<tr class="tr-10">
											<td>{{ "invoicePdf.used_rate"|trans }}:</td>
											<td style="text-align: right;">{{rateBase|format_number}}</td>
											<td style="text-align: right;">Kč</td>
											<td style="text-align: right;"></td>
											<td style="text-align: right;"></td>
										</tr>
										<tr>
											<td>{{ "invoicePdf.amount"|trans }}:</td>
											<td style="text-align: right;">1</td>
											<td style="text-align: right;">EUR</td>
											<td style="text-align: right;"></td>
											<td style="text-align: right;"></td>
										</tr>
									{% endif %}
								</table>
							</td>
						</tr>
					</table>

					<table style="width: 100%;margin-top: 10px;">
						<tr>
							<td style="width: 33.33%;"></td>
							<td style="width: 33.33%;">{{ "invoicePdf.taken_from"|trans }}:
							</td>
							<td style="width: 33.33%;">{{ "invoicePdf.stamp"|trans }}:
							</td>
						</tr>
					</table>
					<p class="p0">{{ "invoicePdf.pohoda_system_message"|trans }}</p>
				</td>
			</tr>
		</table>
	</body>
</html>
